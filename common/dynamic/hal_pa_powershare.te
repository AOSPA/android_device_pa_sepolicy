add_hwservice(hal_pa_powershare_server, hal_pa_powershare_hwservice)
allow hal_pa_powershare_client hal_pa_powershare_hwservice:hwservice_manager find;

# Allow binder communication with platform_app
binder_call(hal_pa_powershare, platform_app)
